<Control name="ListComboBoxDemo">

<content>
    <ListComboBox id="comboBox" />
</content>

<script>
ListComboBoxDemo.prototype.extend({
    initialize: function() {
        this.$comboBox().items([
            "Bird",
            "Cat",
            "Dog",
            "Fish"
        ]);
    }
});
</script>
    
</Control>
